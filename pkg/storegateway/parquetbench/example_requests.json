{
    "label_values": [
        {
            "name": "basic_label_1",
            "label": "label_1",
            "start": 0,
            "end": 1000,
            "matchers": [
                {
                    "type": 0,
                    "name": "label_2",
                    "value": "0"
                },
                {
                    "type": 0,
                    "name": "label_3",
                    "value": "0"
                }
            ],
            "limit": 0
        },
        {
            "name": "with_regex_matcher",
            "label": "__name__",
            "start": 0,
            "end": 1000,
            "matchers": [
                {
                    "type": 2,
                    "name": "job",
                    "value": "prometheus.*"
                }
            ],
            "limit": 100
        }
    ],
    "label_names": [
        {
            "name": "basic_label_names",
            "start": 0,
            "end": 1000,
            "matchers": [
                {
                    "type": 0,
                    "name": "job",
                    "value": "prometheus"
                }
            ],
            "limit": 0
        }
    ],
    "series": [
        {
            "name": "basic_series",
            "min_time": 0,
            "max_time": 1000,
            "matchers": [
                {
                    "type": 0,
                    "name": "__name__",
                    "value": "up"
                }
            ],
            "skip_chunks": true
        },
        {
            "name": "series_with_chunks",
            "min_time": 0,
            "max_time": 1000,
            "matchers": [
                {
                    "type": 0,
                    "name": "__name__",
                    "value": "http_requests_total"
                },
                {
                    "type": 2,
                    "name": "status",
                    "value": "2.."
                }
            ],
            "skip_chunks": false
        }
    ]
}